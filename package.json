{
  "name": "nem",
  "homepage": "https://maks-k.github.io/1up_classic/",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "npm run dev --watch",
    "build": "tsc && vite build",
    "prettier_fix": "npx prettier --write .",
    "lint": "eslint .",
    "dev": "vite",
    "prepare": "husky",
    "deploy": "vite build && gh-pages -d dist"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@esotericsoftware/spine-pixi-v8": "^4.2.74",
    "@pixi/sound": "^6.0.1",
    "@pixi/ui": "^2.2.2",
    "motion": "^12.4.7",
    "pixi.js": "^8.8.1"
  },
  "devDependencies": {
    "@assetpack/core": "^1.4.0",
    "@eslint/js": "^9.21.0",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.25.0",
    "vite": "^6.2.0"
  }
}
